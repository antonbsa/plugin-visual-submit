{
  "requiredSdkVersion": "~0.0.84",
  "name": "VisualSubmit",
  "javascriptEntrypointUrl": "VisualSubmit.js",
  "eventPersistence": {
    "isEnabled": true
  },
  "remoteDataSources": [
    {
      "name": "fileUpload",
      "url": "assets",
      "fetchMode": "onDemand",
      "permissions": [
        "moderator",
        "viewer"
      ]
    }
  ],
  "dataChannels": [
    {
      "name": "submitImage",
      "pushPermission": [
        "all"
      ],
      "replaceOrDeletePermission": [
        "creator",
        "presenter"
      ]
    }
  ]
}
